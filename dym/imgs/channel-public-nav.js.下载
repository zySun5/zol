$(document).ready(function(){$("#J_HeadLinks").find(".is-list").hover(function(){$(this).addClass("hover")},function(){$(this).removeClass("hover")});var c=$("#J_keywords").val();var a="";$("#J_keywords").bind({focus:function(){if(a==""){$(this).val("").css("color","#333")}},blur:function(){if($(this).val()==""){$(this).val(c).css("color","#999");a=""}else{a=$(this).val()}}});$("#J_MainNav").find(".trigger").hover(function(){$(this).addClass("hover")},function(){$(this).removeClass("hover")});$("#J_MoreMenu").hover(function(){$(this).addClass("more-menu-on")},function(){$(this).removeClass("more-menu-on")});if($(".J_Brand").length!=0){$("body").append($('<div id="brandRel" style="display:none;"></div>'));var b=["\u4e13\u533a","\u62a5\u4ef7","\u8bba\u575b","\u7ecf\u9500\u5546","\u7cfb\u5217"];$(".J_Brand a").hover(function(){if(!$(this).attr("data-links")){return}var j=$(this).offset().top+17;var h=$(this).offset().left;var l=$(this).attr("data-links").split(",");var k=$(this).text();var e=l.length;if(e>4){$("#brandRel").addClass("brand-rel-more")}else{$("#brandRel").removeClass("brand-rel-more")}$("#brandRel").css({"position":"absolute","left":h+"px","top":j+"px"});var g="";for(var f=0;f<e;f++){var d=(f==0)?"":"<br>";if(l[f]!==""){g+=d+'<a href="'+l[f]+'">'+k+" "+b[f]+"</a>"}}$("#brandRel").html(g).show()},function(){$("#brandRel").hide()});$("#brandRel").hover(function(){$(this).show()},function(){$(this).hide()})}});
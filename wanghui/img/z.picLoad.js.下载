(function(a){a.fn.picLoad=function(d){var g={checkShow:false,vertical:false};var d=a.extend(g,d);var f=20;var b=this;var e=navigator.userAgent;var c=function(){var h=0;b.each(function(){var j=d.vertical?a.rightoffold(this):a.belowthefold(this);if(!j){a.toshowimg(this)}else{if(h++>f){return false}}});var i=a.grep(b,function(j){return !j.loaded});b=a(i)};if(e.match(/iPhone|iPad|iPod/i)){b.each(function(){a.toshowimg(this)})}else{a(window).bind("scroll",function(){c()});a(window).bind("resize",function(){c()})}if(d.checkShow){c()}};a.toshowimg=function(b){if(!b.loaded&&a(b).attr("data-src")){a(b).attr("src",a(b).attr("data-src")).removeAttr("data-src")}b.loaded=true};a.belowthefold=function(c){var b=a(window).height()+a(window).scrollTop();return b<=a(c).offset().top-1000};a.rightoffold=function(c){var b=a(window).width()+a(window).scrollLeft();return b<=a(c).offset().left}})(jQuery);
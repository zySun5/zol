(function(a){a.fn.lazyNavi=function(f,d,c,b){var e="";a(this).children().mouseover(function(){var g=this;e=setTimeout(function(){if(a(g)==null||a(g).attr("rel")==null){return}if(a(g).hasClass(f)||!a(g).hasClass(d)){if(d){act_class="."+d}else{act_class='[class="'+d+'"]'}var j=a(g).siblings(act_class);if(d){j.removeClass(d)}if(f){j.addClass(f)}var i=j.attr("rel");a("#"+i).hide();var h=a(g).attr("rel");if(f){a(g).removeClass(f)}if(d){a(g).addClass(d)}if(b){a("#"+h).getLazyArea()}a("#"+h).show()}},c)});a(this).children().mouseout(function(){if(e){clearTimeout(e)}})};a.fn.autoNavi=function(e,h,l,j,d,b,i){var m=this;function c(){if(h){act_class="."+h}else{act_class='[class="'+h+'"]'}var o=a(m).children(act_class);if(h){o.removeClass(h)}if(e){o.addClass(e)}var n=o.attr("rel");if(!n){return}a("#"+n).hide();while(1){var p=o.next();if(p.html()==null){p=a(m).children().eq(0)}o=p;if(p.attr("rel")){break}}if(e){p.removeClass(e)}if(h){p.addClass(h)}var q=p.attr("rel");if(j){a("#"+q).getLazyArea()}a("#"+q).show()}var f=setInterval(c,l);function k(){clearInterval(f)}function g(){if(f){clearInterval(f)}f=setInterval(c,l)}if(d){a(d).hover(function(){k()},function(){g()});if(b){a(b).toggle(function(){k();a(this).addClass("stop");a(d).unbind()},function(){g();a(this).removeClass("stop");a(d).hover(function(){k()},function(){g()})})}}if(i){a(i).hover(function(){k()},function(){g()})}};a.fn.getLazyArea=function(){var b=a(this).children("textarea");if(b.length==1){b.hide();var c=b.val();a(this).html(c)}}})(jQuery);
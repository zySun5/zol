$("#J_keywords").zsuggest({offsetX:0,offsetY:8,width:267,isSuggest:true});var _hmt=_hmt||[];var searchVal=$("#J_keywords").val();$("#J_keywords").live("focus",function(){var a=$(this).val();if(a==""||a==searchVal){_hmt.push(["_trackEvent","doc:article:zaChannelSearch:load","load"])}else{_hmt.push(["_trackEvent","doc:article:zaChannelSearchShow:load","load"])}});$("#zSearchSuggest").live("click",function(){if($("#searchResultList").length>0){_hmt.push(["_trackEvent","doc:article:zaChannelSearchShow:click","click"])}if($("#zsearchshow").length>0){_hmt.push(["_trackEvent","doc:article:zaChannelSearch:click","click"])}});
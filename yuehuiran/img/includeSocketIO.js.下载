var IsDisplaySocket=true;var BrowserName=navigator.appName;var BrowserVersion=navigator.appVersion;if(BrowserName=="Microsoft Internet Explorer"){BrowserVersion=BrowserVersion.replace(/[ ]/g,"");if(BrowserVersion.indexOf("MSIE6.0")!=-1){IsDisplaySocket=false}else{if(BrowserVersion.indexOf("MSIE7.0")!=-1){IsDisplaySocket=false}else{if(BrowserVersion.indexOf("MSIE8.0")!=-1){IsDisplaySocket=false}else{if(BrowserVersion.indexOf("MSIE9.0")!=-1){IsDisplaySocket=false}}}}}if(IsDisplaySocket){document.write("<script src='//icon.zol-img.com.cn/comment/js/socket.io.js'><\/script>")};